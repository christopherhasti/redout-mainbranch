{
    "manifest_version": 3,
    "name": "Flashing Image Blocker",
    "version": "1.0",
    "description": "Detects flashing content and blocks it",
    "permissions": ["scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["tracking-min.js", "tracker.js", "content.js"]
      }
    ]
  }
  